{
  "name": "backend",
  "version": "1.0.0",
  "description": "NestJS Backend Application",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed:platforms": "ts-node -r tsconfig-paths/register src/seed-platforms.ts",
    "seed:working-deposit": "ts-node -r tsconfig-paths/register src/seed-working-deposit.ts",
    "seed:all": "ts-node -r tsconfig-paths/register src/seed-all.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
