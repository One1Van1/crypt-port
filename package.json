{
  "name": "srm",
  "version": "1.0.0",
  "description": "SRM Fullstack Application",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:front": "cd frontend && yarn dev",
    "start:back": "cd backend && yarn start:dev",
    "dev": "concurrently \"yarn start:back\" \"yarn start:front\"",
